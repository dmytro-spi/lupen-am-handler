{
  "name": "tile-validator",
  "version": "1.0.0",
  "description": "TypeScript Template with Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "npx --yes http-server ./docs -p 8000",
    "typedoc": "typedoc --out ./docs ./src",
    "play": "npx ts-node src/playground.ts",
    "play:js": "node dist/playground.js",
    "test": "jest",
    "build": "rm -rf dist && tsc && copy-files-from-to"
  },
  "dependencies": {
    "bson": "^6.2.0",
    "jsonschema7": "0.8.0",
    "uuid": "9.0.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "copy-files-from-to": "^3.9.1",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typedoc": "0.25.4"
  },
  "keywords": [],
  "copyFiles": [
    {
      "from": "src/lib",
      "to": "dist/lib"
    }
  ]
}
