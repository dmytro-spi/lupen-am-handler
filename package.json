{
  "name": "tile-validator",
  "version": "1.0.0",
  "description": "TypeScript Template with Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "npx --yes http-server ./docs -p 8000",
    "typedoc": "typedoc --out ./docs ./src",
    "play": "npx ts-node src/playground.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "bson": "^6.2.0",
    "jsonschema7": "0.8.0",
    "uuid": "9.0.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.7",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "typedoc": "0.25.4"
  },
  "husky": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "**/*.ts": "tsc"
  },
  "keywords": []
}
